{
  "name": "digistock",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbo",
    "dev:dev": "env-cmd -f env/.env.dev next dev --turbo",
    "dev:uat": "env-cmd -f env/.env.uat next dev --turbo",
    "dev:production": "env-cmd -f env/.env.production next dev --turbo",
    "build": "env-cmd -f .env next build",
    "build:uat": "env-cmd -f env/.env.uat next build",
    "build:production": "env-cmd -f env/.env.production next build",
    "start": "node .next/standalone/server.js",
    "lint": "next lint",
    "build:icons": "npx tsx ./build-icons.mts"
  },
  "dependencies": {
    "@nextui-org/react": "^2.3.6",
    "@types/nodemailer": "^6.4.16",
    "accept-language": "^3.0.18",
    "chart.js": "^4.4.4",
    "env-cmd": "^10.1.0",
    "eslint": "8.57.0",
    "eslint-config-next": "^14.2.3",
    "framer-motion": "^11.2.6",
    "fs-extra": "^11.2.0",
    "glob": "^10.4.0",
    "next": "v14.2.12",
    "node-html-parser": "^6.1.13",
    "nodemailer": "^6.9.15",
    "react": "^18.3.1",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.3.1",
    "react-use": "^17.5.0",
    "scroll-lock": "^2.1.5",
    "server-only": "^0.0.1",
    "swiper": "^11.1.3"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/negotiator": "^0.6.3",
    "@types/node": "20.12.12",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "@types/scroll-lock": "^2",
    "autoprefixer": "^10.4.19",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "negotiator": "^0.6.3",
    "sass": "^1.77.2",
    "tailwindcss": "3.4.3",
    "typescript": "5.4.5"
  },
  "packageManager": "yarn@4.1.1"
}
